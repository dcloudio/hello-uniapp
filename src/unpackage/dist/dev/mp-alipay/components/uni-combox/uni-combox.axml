<view class="uni-combox data-v-6f115564"><block a:if="{{label}}"><view class="uni-combox__label data-v-6f115564" style="{{(labelStyle)}}"><text class="data-v-6f115564">{{label}}</text></view></block><view class="uni-combox__input-box data-v-6f115564"><input class="uni-combox__input data-v-6f115564" type="text" placeholder="{{placeholder}}" data-event-opts="{{[['input',[['__set_model',['','inputVal','$event',[]]],['onInput',['$event']]]],['focus',[['onFocus',['$event']]]],['blur',[['onBlur',['$event']]]]]}}" value="{{inputVal}}" onInput="__e" onFocus="__e" onBlur="__e"/><uni-icons class="uni-combox__input-arrow data-v-6f115564" vue-id="730fd7f8-1" type="arrowdown" size="14" data-event-opts="{{[['^click',[['toggleSelector']]]]}}" onClick="__e" onVueInit="__l"></uni-icons><block a:if="{{showSelector}}"><view class="uni-combox__selector data-v-6f115564"><scroll-view class="uni-combox__selector-scroll data-v-6f115564" scroll-y="true"><block a:if="{{filterCandidatesLength===0}}"><view class="uni-combox__selector-empty data-v-6f115564"><text class="data-v-6f115564">{{emptyTips}}</text></view></block><view data-event-opts="{{[['tap',[['onSelectorClick',[index]]]]]}}" class="uni-combox__selector-item data-v-6f115564" onTap="__e" a:for="{{filterCandidates}}" a:for-item="item" a:for-index="index" a:key="index"><text class="data-v-6f115564">{{item}}</text></view></scroll-view></view></block></view></view>