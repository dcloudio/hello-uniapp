
map {
	width: 750rpx;
	width: 100%;
	height: 600px;
}
.video {
	width: 100%;
}
.cover-content {
	position: relative;
}
.cover-view {
	position: absolute;
	left: 5px;
	top: 5px;
	width: 375rpx;
	text-align: center;
	background-color: #DDDDDD;
}
.cover-image {
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	margin: auto;
	width: 96px;
	height: 96px;
}

